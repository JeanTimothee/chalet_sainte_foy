<h1><%= @chalet.name %></h1>
<ul class="booking-list">
  <% @bookings.each do |booking| %>
      <li class="booking"><%= booking.start_date %> to <%= booking.end_date %>
      <%= link_to "Delete",
      chalet_booking_path(booking),
      method: :delete,
      data: {confirm: "Are you sure?" } %>
      </li>
  <% end %>
</ul>
